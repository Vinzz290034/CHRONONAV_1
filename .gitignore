# ======================================
# CHRONONAV_WEB_DOSS .gitignore
# ======================================

# OS / Editor junk
.DS_Store
Thumbs.db
*.log
*.tmp
*.swp
*.bak
*.lock

# Composer dependencies
/vendor/

# User uploads (images, docs, etc.)
/uploads/

# Environment & config files
.env
*.env
.env.local
.env.*.local

# PHPStorm / VSCode / IDE settings
.idea/
.vscode/

# Node.js (if you later use npm for frontend)
/node_modules/
npm-debug.log
yarn-error.log

# Cache & sessions
/cache/
*.cache
*.session
*.sess

# PHPUnit / coverage reports
.phpunit.result.cache
coverage/

# Backup / build / dist
/build/
/dist/
*.tar
*.zip
*.gz

# XAMPP / system-specific
htdocs/error_log
config/db_connect.php
